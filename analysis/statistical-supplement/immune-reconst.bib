  author = {Ferrer, Lo{\"i}c and Rondeau, Virginie and Dignam, James and Pickles, Tom and {Jacqmin-Gadda}, H{\'e}l{\`e}ne and {Proust-Lima}, C{\'e}cile},
  year = {2016},
