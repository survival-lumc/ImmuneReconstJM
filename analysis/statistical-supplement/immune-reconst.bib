@article{ferrerIndividualDynamicPredictions2019,
  title = {Individual Dynamic Predictions Using Landmarking and Joint Modelling: {{Validation}} of Estimators and Robustness Assessment},
  shorttitle = {Individual Dynamic Predictions Using Landmarking and Joint Modelling},
  author = {Ferrer, Lo{\"i}c and Putter, Hein and {Proust-Lima}, C{\'e}cile},
