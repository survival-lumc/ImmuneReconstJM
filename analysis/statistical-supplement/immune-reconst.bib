@article{pajouheshniaAccountingTimedependentTreatment2020,
  title = {Accounting for Time-Dependent Treatment Use When Developing a Prognostic Model from Observational Data: {{A}} Review of Methods},
  shorttitle = {Accounting for Time-Dependent Treatment Use When Developing a Prognostic Model from Observational Data},
  author = {Pajouheshnia, Romin and Schuster, Noah A. and Groenwold, Rolf H. H. and Rutten, Frans H. and Moons, Karel G. M. and Peelen, Linda M.},
  year = {2020},
  journal = {Statistica Neerlandica},
  volume = {74},
  number = {1},
  pages = {38--51},
  issn = {1467-9574},
  doi = {10.1111/stan.12193},
  abstract = {Failure to account for time-dependent treatment use when developing a prognostic model can result in biased future predictions. We reviewed currently available methods to account for treatment use when developing a prognostic model. First, we defined the estimands targeted by each method and examined their mechanisms of action with directed acyclic graphs (DAGs). Next, methods were implemented in data from 1,906 patients; 325 received selective {$\beta$}-blockers (SBBs) during follow-up. We demonstrated seven Cox regression modeling strategies: (a) ignoring SBB treatment; (b) excluding SBB users or (c) censoring them when treated; (d) inverse probability of treatment weighting after censoring (IPCW), including SBB treatment as (e) a binary or (f) a time-dependent covariate; and (g) marginal structural modeling (MSM). Using DAGs, we demonstrated IPCW and MSM have the best properties and target a similar estimand. In the case study, compared to (a), approaches (b) and (e) provided predictions that were 1\% and 2\% higher on average. Performance (c-statistic, Brier score, calibration slope) varied minimally between approaches. Our review of methods confirmed that ignoring treatment is theoretically inferior, but differences between the prediction models obtained using different methods can be modest in practice. Future simulation studies and applications are needed to assess the value of applying IPCW or MSM to adjust for treatments in different treatment and disease settings.},
  copyright = {\textcopyright{} 2019 The Authors. Statistica Neerlandica Published by John Wiley \& Sons, Ltd. on behalf of VVS.},
  langid = {english},
  keywords = {Cox regression,inverse probability weights,marginal structural model,prediction,prognosis},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/stan.12193},
  file = {C\:\\Users\\efbonneville\\Zotero\\storage\\HQ3PL28D\\Pajouheshnia et al. - 2020 - Accounting for time-dependent treatment use when d.pdf;C\:\\Users\\efbonneville\\Zotero\\storage\\MYVMH5GX\\stan.html}
}

@article{papageorgiouIndividualizedDynamicPrediction2019,
  title = {Individualized Dynamic Prediction of Survival with the Presence of Intermediate Events},
  author = {Papageorgiou, Grigorios and Mokhles, Mostafa M. and Takkenberg, Johanna J. M. and Rizopoulos, Dimitris},
  year = {2019},
  journal = {Statistics in Medicine},
  volume = {38},
  number = {30},
  pages = {5623--5640},
  issn = {1097-0258},
  doi = {10.1002/sim.8387},
  abstract = {Often, in follow-up studies, patients experience intermediate events, such as reinterventions or adverse events, which directly affect the shapes of their longitudinal profiles. Our work is motivated by two studies in which such intermediate events have been recorded during follow-up. In both studies, we are interested in the change of the longitudinal evolutions after the occurrence of the intermediate event and in utilizing this information to improve the accuracy of dynamic prediction of their risk. To achieve so, we propose a flexible joint modeling framework for longitudinal and time-to-event data, which includes features of the intermediate event as time-varying covariates in both the longitudinal and survival submodels. We consider a set of joint models that postulate different effects of the intermediate event in the longitudinal profile and the risk of the clinical endpoint, with different formulations for the association structure while allowing its functional form to change after the occurrence of the intermediate event. Based on these models, we derive dynamic predictions of conditional survival probabilities which are adaptive to different scenarios with respect to the occurrence of the intermediate event. We evaluate the predictive accuracy of these predictions with a simulation study using the time-dependent area under the receiver operating characteristic curve and the expected prediction error adjusted to our setting. The results suggest that accounting for the changes in the longitudinal profiles and the instantaneous risk for the clinical endpoint is important, and improves the accuracy of the dynamic predictions.},
  copyright = {\textcopyright{} 2019 The Authors. Statistics~in~Medicine Published by John Wiley \& Sons Ltd.},
  langid = {english},
  keywords = {dynamic predictions,joint modeling,longitudinal data,survival data},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/sim.8387},
  file = {C\:\\Users\\efbonneville\\Zotero\\storage\\GEBQN7BL\\Papageorgiou et al. - 2019 - Individualized dynamic prediction of survival with.pdf}
}

